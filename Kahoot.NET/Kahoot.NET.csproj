<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <SignAssembly>False</SignAssembly>
    <PackageProjectUrl>https://github.com/ac111897/Kahoot.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ac11897/Kahoot.NET</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Kahoot,Library</PackageTags>
    <NeutralLanguage>en-NZ</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Flee" Version="1.2.2" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="OneOf" Version="3.0.211" />
    <PackageReference Include="Towel" Version="1.0.40" />
  </ItemGroup>

  <ItemGroup>
    <EditorConfigFiles Remove="F:\repos\Kahoot.NET\Kahoot.NET\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <None Include="F:\repos\Kahoot.NET\Kahoot.NET\.editorconfig" />
  </ItemGroup>

</Project>
